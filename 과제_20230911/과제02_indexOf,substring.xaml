    <Assign sap2010:Annotation.AnnotationText="Substring을 이용하여 233인덱스 부터  8글자를 추출한다." DisplayName="Assign_assignment02" sap:VirtualizedContainerService.HintSize="434,113" sap2010:WorkflowViewState.IdRef="Assign_3">
